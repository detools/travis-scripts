  },
  "homepage": "https://github.com/detools/travis-scripts#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "command-exists": "^1.2.8",
    "esm": "^3.2.18",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.11",
    "luxon": "^1.11.4"
  },
  "devDependencies": {
    "@detools/eslint-config": "^1.2.0",
